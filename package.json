{
  "name": "nexio-ai",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "build:prod": "nest build",
    "nest:start": "nest start ./src/main.ts",
    "nest:start:dev": "nest start --watch",
    "nest:start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main.js",
    "start:dev:bun": "bun run ./src/main.ts",
    "watch:bun": "bun run --watch ./src/main.ts",
    "build:bun": "cross-env NODE_ENV=production bun build --external=class-validator --external=@nestjs/sequelize --external=@mikro-orm/core --external=@nestjs/mongoose --external=mock-aws-s3 --external=hbs --external=aws-sdk --external=nock --external=@nestjs/websockets/socket-module --external=class-transformer --external=@nestjs/microservices --target=bun --minify ./dist/main.js --outdir=dist --format esm ./src/main.ts",
    "typeorm": "typeorm-ts-node-esm",
    "migration:generate": "bun run typeorm migration:generate -d ormconfig.ts",
    "migration:create": "bun run typeorm migration:create -d ormconfig.ts",
    "generate": "nest g -c awesome-nestjs-schematics --no-spec",
    "migration:revert": "bun run typeorm migration:revert",
    "schema:drop": "bun run typeorm schema:drop",
    "watch:dev": "nest start --watch ./src/main.ts",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:cov": "cross-env NODE_ENV=test jest --coverage",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json",
    "docs:dev": "vuepress dev -p 7070",
    "docs:build": "cross-env DEPLOY_ENV=gh-pages vuepress build",
    "docs:deploy": "yarn docs:build && gh-pages -d .vuepress/dist",
    "prepare": "husky",
    "release": "release-it",
    "commit": "git-cz"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.5",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.10",
    "@nestjs/cqrs": "^11.0.2",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/microservices": "^11.1.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.10",
    "@nestjs/swagger": "^11.0.5",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "bcryptjs": "^3.0.2",
    "class-transformer": "~0.5.1",
    "class-validator": "~0.14.1",
    "commitizen": "^4.3.1",
    "compression": "^1.8.0",
    "express": "^5.0.1",
    "git-cz": "^4.9.0",
    "helmet": "^8.0.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mime-types": "^3.0.0",
    "morgan": "^1.10.0",
    "nestjs-cls": "^5.4.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.13.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "tslib": "^2.8.1",
    "typeorm": "0.3.20",
    "typeorm-transactional": "~0.5.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.23.0",
    "@nestjs/cli": "^11.0.4",
    "@nestjs/testing": "^11.0.10",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.11.1",
    "@swc/plugin-transform-imports": "^7.0.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/compression": "^1.7.5",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/lodash": "^4.17.15",
    "@types/mime-types": "^2.1.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.13.5",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "awesome-nestjs-schematics": "^10.1.1",
    "cross-env": "^7.0.3",
    "esbuild": "^0.25.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-base-typescript": "^1.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "gh-pages": "^6.3.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.28.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
